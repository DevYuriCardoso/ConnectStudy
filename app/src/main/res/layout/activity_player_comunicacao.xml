        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aula 01"
        app:layout_constraintStart_toStartOf="@+id/btn_aula01"
        app:layout_constraintTop_toBottomOf="@+id/btn_aula01" />

    <TextView
        android:id="@+id/txt_aula03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aula 03"
        app:layout_constraintEnd_toEndOf="@+id/btn_aula03"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_aula03"
        app:layout_constraintTop_toBottomOf="@+id/btn_aula03" />

    <TextView
        android:id="@+id/txt_aula04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aula 04"
        app:layout_constraintEnd_toEndOf="@+id/btn_aula04"
        app:layout_constraintStart_toStartOf="@+id/btn_aula04"
        app:layout_constraintTop_toBottomOf="@+id/btn_aula04" />

    <TextView
        android:id="@+id/txt_aula02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aula 02"
        app:layout_constraintEnd_toEndOf="@+id/btn_aula02"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_aula02"
        app:layout_constraintTop_toBottomOf="@+id/btn_aula02" />

    <ImageView
        android:id="@+id/btn_aula01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/btn_aula02"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideli6ne6"
        app:srcCompat="@drawable/ps" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <ImageView
        android:id="@+id/btn_aula02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/btn_aula03"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_aula01"
        app:layout_constraintTop_toTopOf="@+id/guideli6ne6"
        app:srcCompat="@drawable/ps" />

    <ImageView
        android:id="@+id/btn_aula03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/btn_aula04"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_aula02"
        app:layout_constraintTop_toTopOf="@+id/guideli6ne6"
        app:srcCompat="@drawable/ps" />

    <ImageView
        android:id="@+id/btn_aula04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
